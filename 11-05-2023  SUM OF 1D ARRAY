# Given an array nums. We define a running sum of an array as runningSum[i] = sum(nums[0]â€¦nums[i]).
****    https://leetcode.com/problems/running-sum-of-1d-array/description/    *****
class Solution:
    def runningSum(self, nums: List[int]) -> List[int]:
        n=len(nums)
        ps=[0]*(n)
        ps[0]=nums[0]
        for i in range(1,n):
            ps[i]=ps[i-1]+nums[i]
        return ps
        
