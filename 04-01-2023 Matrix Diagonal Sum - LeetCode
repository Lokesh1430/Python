class Solution:
    def diagonalSum(self, mat: List[List[int]]) -> int:
        n=len(mat)
        s=0
        for i in range(n):
            for j in range(n):
                if i==j:
                    s=s+mat[i][j]
                elif i+j==n-1:
                    s=s+mat[i][j]
        return s
        
        
        
 OUTPUT:
 IN:
    [[1,2,3],[4,5,6],[7,8,9]]
 OUT:
     25
     
     
     
     
     
     
     
     
     
     
 
